TreeNode current; TreeNode solve(TreeNode root) {         TreeNode Dummy = new TreeNode(-1);         current = Dummy;         Dfs(root);         TreeNode head = Dummy.right;         current.right = head;         return head; }  public void Dfs(TreeNode node) {         if (node == null) return;         Dfs(node.left);         current.right = node;         node.left = current;         current = current.right;         Dfs(node.right); }
